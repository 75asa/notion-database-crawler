  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/75asa/notion-database-crawler.git"
  },
  "scripts": {
    "ts-node": "ts-node",
    "test": "jest",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint-fix": "yarn lint --fix && prettier --write \"./{__tests__,src}/**/*.{js,ts}\"",
    "build": "tsc -p .",
    "postinstall": "yarn build",
    "generate": "yarn prisma generate",
    "migrate": "yarn prisma migrate dev",
    "migrate:reset": "yarn prisma migrate reset",
    "migrate:deploy": "yarn prisma migrate deploy",
    "deploy": "tsc . && node dist/index.js"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.0",
    "@prisma/client": "3.0.2",
    "@slack/web-api": "^6.4.0",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "jsx-slack": "^4.3.0",
    "shallow-equal-object": "^1.1.1",
    "toad-scheduler": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "prisma": "3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "engines": {
    "yarn": "1.*",
    "node": "16.x"
  }
}
